\begin_layout Standard
Equations
\begin_inset Formula 
\[
\Delta A_{t}=\mu_{t}A_{t-1}+\nu I_{t-1}-\gamma_{A}A_{t-1}-\iota A_{t}+\eta_{A,t}
\]

\end_inset


\begin_inset Formula 
\[
\Delta I_{t}=\iota A_{t-1}-\delta I_{t-1}-\gamma_{I}I_{t-1}+\eta_{I,t}
\]

\end_inset


\begin_inset Formula 
\[
\Delta C_{t}=\delta I_{t-1}-\phi C_{t-1}-\gamma_{C}C_{t-1}+\eta_{C,t}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\Delta D_{t} & =\phi C_{t-1}+\eta_{D,t}
\end{align*}

\end_inset


\begin_inset Formula 
\[
\Delta R_{t}=\gamma_{I}I_{t-1}+\gamma_{C}C_{t-1}+\eta_{R,t}
\]

\end_inset


\end_layout

\begin_layout Standard
where 
\begin_inset Formula 
\[
\var(\eta_{\bullet,t}|\F_{t})=\text{rest of the r.h.s.}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $A$
\end_inset

 and 
\begin_inset Formula $C$
\end_inset

 unobserved.
\end_layout

\begin_layout Standard
We have
\begin_inset Formula 
\[
A_{t}=\underbrace{(1+\mu_{t}-\gamma_{A}-\iota)}_{\theta_{t}}A_{t-1}+\nu I_{t-1}+\eta_{A,t}
\]

\end_inset


\end_layout

\begin_layout Standard
we rewrite 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline*}
A_{t}=\theta_{t}A_{t-1}+\nu I_{t-1}+\eta_{A,t}=\theta_{t}(\theta_{t-1}A_{t-2}+\nu I_{t-2}+\eta_{A,t-1})+\nu I_{t-1}+\eta_{A,t}\\
=Z_{t}+\epsilon_{t},\qquad Z_{t}=(\theta_{1}\times\dots\times\theta_{t})\alpha_{0}+\nu\sum_{\tau=1}^{t-1}(\theta_{\tau+2}\times\dots\times\theta_{t})I_{\tau},\qquad\epsilon_{t}=\sum_{\tau=1}^{t}(\theta_{\tau+1}\times\dots\times\theta_{t})\eta_{A,t}
\end{multline*}

\end_inset

where 
\begin_inset Formula $\alpha_{0}=A_{0}$
\end_inset

 (to be estimated), and Similarly,
\begin_inset Formula 
\[
C_{t}=\underbrace{(1-\phi-\gamma_{C})}_{\omega}C_{t-1}+\delta I_{t-1}+\eta_{C,t}
\]

\end_inset

so 
\begin_inset Formula 
\[
C_{t}=Y_{t}+\varepsilon_{t},\qquad Y_{t}=\sum_{\tau=1}^{t-1}\omega^{t-\tau-1}\delta I_{\tau},\qquad\varepsilon_{t}=\omega^{t-\tau}\eta_{C,t}
\]

\end_inset


\end_layout

\begin_layout Standard
Both 
\begin_inset Formula $Z$
\end_inset

 and Y may be evaluated recursively (assuming 
\begin_inset Formula $I_{0}=0$
\end_inset

: 
\begin_inset Formula 
\[
Z_{0}=\alpha_{0},Z_{1}=\theta_{1}\alpha_{0}+\nu I_{0},Z_{2}=\theta_{2}\theta_{1}\alpha_{0}+\theta_{2}\nu I_{0}+\nu I_{1},\dots Z_{t}=\theta_{t}Z_{t-1}+\nu I_{t-1}
\]

\end_inset

and 
\begin_inset Formula 
\[
Y_{0}=0,Y_{1}=0,Y_{2}=\sigma I_{1},Y_{3}=\omega\delta I_{1}+\delta I_{2}\qquad Y_{t}=\omega Y_{t-1}+\delta I_{t-1}
\]

\end_inset


\end_layout

\begin_layout Standard
This gives 
\begin_inset Formula 
\[
I_{t}=\underbrace{(1-\delta-\gamma_{I})}_{\sigma}I_{t-1}+\iota Z_{t-1}+e_{t},\qquad e_{t}=\iota\epsilon_{t-1}+\eta_{I,t}
\]

\end_inset


\begin_inset Formula 
\[
D_{t}=D_{t-1}+\phi Y_{t-1}+f_{t},\qquad f_{t}=\phi\varepsilon_{t-1}+\eta_{D,t}
\]

\end_inset


\begin_inset Formula 
\[
R_{t}=R_{t-1}+\gamma_{I}I_{t-1}+\gamma_{C}Y_{t-1}+g_{t},\qquad g_{t}=\gamma_{C}\varepsilon_{t-1}+\eta_{R,t}
\]

\end_inset

It is not easy to compute 
\begin_inset Formula $\var(e,f,g|\G_{t-1})$
\end_inset

 (possible in normal case via well known formula for conditional distribution,
 no time here for this).
 So we dare to estimate the variances by 
\begin_inset Formula $\var(\bullet_{t}|\F_{\t-1}),$
\end_inset

 which equals to those of etas.
\end_layout

